    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.39",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.8",
    "@types/react-router-dom": "^5.1.5",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.5.0",
    "codemirror": "^5.53.2",
    "env-cmd": "^10.0.1",
    "express": "^4.17.1",
