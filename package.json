  "scripts": {
    "start:prod": "npm run prebuild && npm run build && env-cmd -f ./src/server/config/prod.env node src/server/distServer.js",
    "start:dev": "npm-run-all --parallel open:server",
    "open:server": "env-cmd -f ./src/server/config/dev.env nodemon src/server/srcServer.js -e js,jsx",
    "start": "set PORT=3008 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
