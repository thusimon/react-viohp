{
  "name": "violin-assist",
  "version": "1.0.0",
  "description": "Help violin beginners",
  "author": "Lu Xu",
  "license": "MIT",
  "engines": {
    "node": ">=16.13.1"
  },
  "scripts": {
    "start:prod": "npm run build && env-cmd -f ./src/server/config/prod.env node src/server/distServer.js",
    "start:dev": "concurrently --kill-others \"npm run webpack:dev\" \"npm run open:server\"",
    "webpack:dev": "nodemon --watch src -e js,jsx,ts,tsx --ignore src/server src/server/srcWebpack.js",
    "open:server": "env-cmd -f ./src/server/config/dev.env nodemon --watch src/server -e js,jsx,ts,tsx src/server/srcServer.js",
    "start": "node src/server/distServer.js",
    "postinstall": "npm run build",
    "build": "node tools/build.js"
  },
  "dependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
