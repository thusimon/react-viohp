{
  "name": "violin-assist",
  "version": "1.0.0",
  "description": "Help violin beginners",
  "author": "Lu Xu",
  "license": "MIT",
  "scripts": {
    "build-babel": "babel -d toolsTranspile tools/*.js -s",
    "start": "npm run build",
    "start:dev": "npm-run-all --parallel open:server lint:watch",
    "open:server": "node toolsTranspile/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec toolsTranspile/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "prebuild": "npm-run-all clean-dist build-babel",
    "build": "node toolsTranspile/build.js",
    "postbuild": "node toolsTranspile/distServer.js"
  },
  "dependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.2.1",
    "codemirror": "^5.49.0",
    "compression": "^1.7.3",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.3.1",
    "mini-css-extract-plugin": "0.4.1",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.3",
    "open": "^6.4.0",
    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-codemirror2": "^6.0.0",
    "react-dom": "^16.9.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "svg-inline-loader": "^0.8.0",
    "svg-inline-react": "^3.1.0",
    "toastr": "^2.1.4",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.3"
  },
  "devDependencies": {
    "eslint": "^5.2.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-watch": "^4.0.2",
    "expect": "^23.4.0",
    "jsdom": "^11.12.00",
    "mocha": "^5.2.0",
    "nock": "^9.4.4",
    "redux-mock-store": "^1.5.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3"
  },
  "mocha": {
    "testURL": "http://localhost"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thusimon/react-viohp"
  }
}
