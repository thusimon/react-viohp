    "mini-css-extract-plugin": "0.4.1",
    "mocha": "^5.2.0",
    "nock": "^9.4.4",
    "npm-run-all": "^4.1.3",
    "open": "^0.0.5",
    "react-addons-test-utils": "^15.6.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
