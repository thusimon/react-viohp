    "codemirror": "^5.53.2",
    "compression": "^1.7.4",
    "concurrently": "^6.5.1",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "d3": "^7.2.1",
    "env-cmd": "^10.0.1",
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.5.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.4",
    "node-sass": "^6.0.1",
