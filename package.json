    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.2.1",
    "jquery": "^3.3.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
