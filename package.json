{
  "name": "violin-assist",
  "version": "1.0.0",
  "description": "Help violin beginners",
  "author": "Lu Xu",
  "license": "MIT",
  "scripts": {
    "start": "npm run prebuild && npm run build && node src/server/distServer.js",
    "start:prod": "npm run prebuild && npm run build && env-cmd -f ./src/server/config/prod.env node src/server/distServer.js",
    "start:dev": "npm-run-all --parallel open:server lint:watch",
    "open:server": "env-cmd -f ./src/server/config/dev.env nodemon src/server/srcServer.js -e js,jsx",
    "lint": "./node_modules/.bin/eslint webpack.config.*.js src tools",
    "lint:watch": "./node_modules/.bin/esw --watch webpack.config.*.js src tools",
    "test": "mocha --reporter spec tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "prebuild": "npm-run-all clean-dist",
    "build": "node tools/build.js"
  },
  "dependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.2.1",
    "codemirror": "^5.49.0",
    "compression": "^1.7.3",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^1.0.0",
    "env-cmd": "^10.0.1",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.5.1",
    "mini-css-extract-plugin": "^0.8.0",
    "mongoose": "^5.7.7",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-codemirror2": "^6.0.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "svg-inline-loader": "^0.8.0",
    "svg-inline-react": "^3.1.0",
    "terser-webpack-plugin": "^2.2.1",
    "toastr": "^2.1.4",
    "url-loader": "^1.0.1",
    "validator": "^12.0.0",
    "webpack": "^4.16.3"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-watch": "^6.0.1",
    "expect": "^23.4.0",
    "jsdom": "^11.12.00",
    "mocha": "^5.2.0",
    "nock": "^9.4.4",
    "redux-mock-store": "^1.5.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3"
  },
  "mocha": {
    "testURL": "http://localhost"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thusimon/react-viohp"
  }
}
